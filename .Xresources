! Xft resources --------------------------------------------------------------
! WILL be overrided by fonts.conf
Xft.dpi:		96
Xft.antialias:	true
Xft.rgba:		rgb
Xft.hinting:	true
Xft.hintstyle:	hintslight

! terminal colors ------------------------------------------------------------

!! tangoesque scheme
!*background: #111111
!*foreground: #babdb6
!! Black (not tango) + DarkGrey
!*color0:  #000000
!*color8:  #555753
!! DarkRed + Red
!*color1:  #ff6565
!*color9:  #ff8d8d
!! DarkGreen + Green
!*color2:  #93d44f
!*color10: #c8e7a8
!! DarkYellow + Yellow
!*color3:  #eab93d
!*color11: #ffc123
!! DarkBlue + Blue
!*color4:  #204a87
!*color12: #3465a4
!! DarkMagenta + Magenta
!*color5:  #ce5c00
!*color13: #f57900
!!DarkCyan + Cyan (both not tango)
!*color6:  #89b6e2
!*color14: #46a4ff
!! LightGrey + White
!*color7:  #cccccc
!*color15: #ffffff


!black
*color0:    #393939
*color8:    #121212

!red
*color1:    #DA3955
*color9:    #ff4775

!green
*color2:    #308888
*color10:   #53A6A6

!yellow
*color3:    #54777d
*color11:   #a94f63

!blue
*color4:    #6d9cbe
*color12:   #91C1E3

!magenta
*color5:    #6f4484
*color13:   #915eaa

!cyan
*color6:    #2B7694
*color14:   #47959E

!white
*color7:    #D6D6D6
*color15:   #A3A3A3

! urxvt configurations -------------------------------------------------------
URxvt.foreground:           #D6D6D6
URxvt.background:           #050505
URxvt.scrollBar: false
! For PC
URxvt.font: xft:Terminus:size=10
! For rMBP
!URxvt.font: xft:Inconsolata:size=12
!URxvt.letterSpace: -1
