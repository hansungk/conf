! Xft resources --------------------------------------------------------------
! WILL be overrided by fonts.conf
Xft.dpi:		96
Xft.antialias:	true
Xft.rgba:		rgb
Xft.hinting:	true
Xft.hintstyle:	hintslight

! terminal colors ------------------------------------------------------------
#include "/home/stephen/.colors/base16-ocean.dark.xresources"
!URxvt*background: #002b36

! urxvt configurations -------------------------------------------------------
URxvt.scrollBar: false
! For PC
!URxvt.font: -*-gohufont-medium-r-normal--11-*-*-*-*-*-*-*,-*-stlarch-*-*-normal-*-*-*-*-*-*-*-*-*,-*-nanumgothic-medium-r-normal-*-12-*-*-*-*-*-*-*
URxvt.font: xft:DejaVu Sans Mono:size=10,xft:NanumGothicCoding:size=11
URxvt.letterSpace: -1
!URxvt*.depth: 24

!URxvt.transparent: true
!URxvt.shading: 20
!URxvt.blurRadius: 30
!URxvt.fading: 30
!URxvt.internalBorder: 10

URxvt*.inputMethod: ibus
URxvt*.preeditType: OverTheSpot
URxvt*.imLocale: ko_KR.UTF-8
