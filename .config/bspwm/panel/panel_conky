background yes
out_to_console yes
out_to_x no
update_interval 0.2

TEXT
S${if_mpd_playing}%{F\#FFFF5500}${mpd_artist}%{F-} - ${mpd_album} - ${mpd_title}   ${endif}%{F\#FFFF5500}Vol%{F-} ${exec amixer get Master | awk 'NR==5 {print $4}' | tr -d '['-']' | tr -d '%'}%%   %{F\#FFFF5500}Time%{F-} ${time %I:%M %P}
